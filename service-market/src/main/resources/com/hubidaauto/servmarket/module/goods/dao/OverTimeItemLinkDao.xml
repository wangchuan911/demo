<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hubidaauto.servmarket.module.goods.dao.OverTimeItemLinkDao">

    <select id="get"
            parameterType="long"
            resultType="com.hubidaauto.servmarket.module.goods.entity.OverTimeItemVO">
        select * from OVER_TIME_ITEM_LINK a where a.ITEM_ID=#{value}
    </select>


</mapper>
